{
  "version": 2,
  "public": true,
  "cleanUrls": false,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/Music/.*\\.mp3",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Content-Type": "audio/mpeg"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "*"
        }
      ]
    }
  ]
}
