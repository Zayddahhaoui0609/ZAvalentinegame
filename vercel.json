{
  "version": 2,
  "public": true,
  "cleanUrls": false,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/Music/.*\\.mp3",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Content-Type": "audio/mpeg"
      }
    },
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "*"
      },
      "continue": true
    }
  ]
}
